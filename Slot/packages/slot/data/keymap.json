{
    "app.commandPalette": "f1",
    "app.toggleCommandBar": "f2",

    "view.newView": "ctrl+shift+n",
    "view.closeView": "ctrl+shift+w",
    "view.toggleFullScreen": "f11",
    
    "file.newfile": "ctrl+n",
    "file.openfile": "ctrl+o",
    "file.closeFile": "ctrl+w",
    "file.openrecentfile": "ctrl+shift+o",
    "file.savefile": "ctrl+s",
    "file.switchbuffer": "ctrl+tab",

    "editor.showsearch": "ctrl+f",
    "editor.searchnext": "f3",
    "editor.searchprevious": "f4",
    "editor.searchselect": "ctrl+f3",
    "editor.searchregex": "alt+r",
    "editor.searchcasesensitive": "alt+c",
    "editor.searchwholeword": "alt+w",

    "editor.addCaretAbove": "ctrl+alt+up",
    "editor.addCaretBelow": "ctrl+alt+down",
    "editor.gotoLine": "ctrl+g",
    "editor.insertlinebelow": "ctrl+enter",
    "editor.insertlineabove": "ctrl+shift+enter",
    "editor.deleteline": "ctrl+shift+k",
    "editor.togglefolding": "ctrl+k",
    "editor.autocomplete": "ctrl+space",
    //"editor.followlink": "ctrl+click",
    "editor.deletewordback": "ctrl+back",
    "editor.deleteword": "ctrl+del",
    "editor.scrollup": "ctrl+up",
    "editor.scrolldown": "ctrl+down",
    "editor.redo": "ctrl+y",
    "editor.undo": "ctrl+z",
    "editor.selectall": "ctrl+a",
    "editor.paste": "ctrl+v",
    "editor.cut": "ctrl+x",
    "editor.copy": "ctrl+c",
    "editor.setcaret": [ "click", "alt+click" ],
    "editor.addcaret": "ctrl+click",
    "editor.selectnormal": [ "move+click", "ctrl+move+click" ],
    "editor.selectblock": "alt+move+click",
    "editor.selectword": [ "doubleclick", "ctrl+d" ],
    "editor.selectwordadd": "ctrl+doubleclick",
    "editor.unindent": "shift+tab",
    "editor.indent": "tab",
    "editor.clearselections": "esc",
    "editor.left": "left",
    "editor.right": "right",
    "editor.up": "up",
    "editor.down": "down",
    "editor.home": "home",
    "editor.end": "end",
    "editor.newline": "enter",
    "editor.deleteback": "back",
    "editor.delete": "del",
    "editor.pagedown": "pagedown",
    "editor.pageup": "pageup",
    "editor.extendleft": "shift+left",
    "editor.extendright": "shift+right",
    "editor.extendup": "shift+up",
    "editor.extenddown": "shift+down",
    "editor.extendend": "shift+end",
    "editor.extendhome": "shift+home",
    "editor.wordleft": "ctrl+left",
    "editor.wordright": "ctrl+right",
    "editor.extendwordright": "ctrl+shift+right",
    "editor.extendwordleft": "ctrl+shift+left",
    "editor.extendpagedown": "shift+pagedown",
    "editor.extendpageup": "shift+pageup",
    "editor.documenthome": "ctrl+home",
    "editor.documentend": "ctrl+end",
    "editor.extenddocumenthome": "ctrl+shift+home",
    "editor.extenddocumentend": "ctrl+shift+end",

    "buffer.toggleOvertype": "ins",
    "buffer.toggleWordwrap":  "alt+z"
}
