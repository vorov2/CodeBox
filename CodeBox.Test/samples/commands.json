        "alias": "e-sa",
        "title": "Editor: Select all"
    },
    {
        "key": "editor.deleteline",
        "alias": "e-ld",
        "title": "Editor: Delete current line (or selected lines)"
    },
    {
        "key": "editor.deleteline",
        "alias": "e-ld",
        "title": "Editor: Delete current line (or selected lines)"
    },
    {
        "key": "editor.insertlinebelow",
        "alias": "e-lib",
        "title": "Editor: Insert line below caret position"
    },
    {
        "key": "editor.insertlineabove",
        "alias": "e-lia",
        "title": "Editor: Insert line above caret position"
