[
    {
        "key": "test.commandpalette",
        "alias": "?",
        "title": "Command Palette",
        "arguments": [
            {
                "name": "commandName",
                "valueProvider": "values.commands"
            }
        ]
    },



    //File
    {
        "key": "file.openfile",
        "alias": "f-o",
        "title": "Open a file",
        "arguments": [
            {
                "name": "fileName",
                "valueProvider": "values.systempath",
                "affinity": "FilePath"
            },
            {
                "name": "encoding",
                "valueProvider": "values.encoding",
                "optional": true
            }
        ]
    },
    {
        "key": "file.openrecentfile",
        "alias": "f-or",
        "title": "Open a recent file",
        "arguments": [
            {
                "name": "fileName",
                "valueProvider": "values.recentdocs"
            }
        ]
    },
    {
        "key": "file.openmodifiedfile",
        "alias": "f-om",
        "title": "Open a modified file",
        "arguments": [
            {
                "name": "fileName",
                "valueProvider": "values.modifieddocs"
            }
        ]
    },
    {
        "key": "file.savefile",
        "alias": "f-s",
        "title": "Save file",
        "arguments": [
            {
                "name": "fileName",
                "optional": true
            }
        ]
    },
    {
        "key": "file.newfile",
